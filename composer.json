{
    "name": "tpximpact/backend-challenge",
    "description": "TPXimpact Backend Challenge",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Root\\BackendChallenge\\": "src/"
        }
    },
    "authors": [
